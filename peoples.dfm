  object qry_Brig: TRxQuery
    DatabaseName = 'DBN_AVAR'
    SQL.Strings = (
      'select id,name_r,DOLGN,UID,PRAVA,DEL,'
      'rtrim(name_r)||'#39'    '#39'||rtrim(DOLGN) as name_r_full'
      'from s_brig'
      
        'where ((Dolgn LIKE '#39'%'#1041#1056#1048#1043#1040#1044#1048#1056'%'#39') or (Dolgn like '#39'%'#1052#1040#1057#1058#1045#1056'%'#39') or (' +
        'id=-1))'
      'and del = '#39'-'#39
      'order by name_r')
    Macros = <>
    Left = 80
    Top = 83
  end
  object Qry_select: TRxQuery
    DatabaseName = 'DBN_AVAR'
    SQL.Strings = (
      'select b.id, b.name_r '
      'from s_team t, s_brig b'
      'where (t.chief=:chief) and (t.labour=b.id)'
      'and (b.del='#39'-'#39')'
      'order by b.name_r')
    Macros = <>
    Left = 376
    Top = 219
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'chief'
        ParamType = ptUnknown
      end>
  end
